<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>LoginMaster</display-name>
  <servlet>
    <servlet-name>faith</servlet-name>
    <servlet-class>com.ex.AddServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>faith</servlet-name>
    <url-pattern>/add</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>faithsq</servlet-name>
    <servlet-class>com.ex.SqServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>faithsq</servlet-name>
    <url-pattern>/sq</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>demosession</servlet-name>
    <servlet-class>com.ex.SessionDemo</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>demosession</servlet-name>
    <url-pattern>/test</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>selectdb</servlet-name>
    <servlet-class>com.ex.SelectDB</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>selectdb</servlet-name>
    <url-pattern>/select</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>FirstServlet</servlet-name>
    <servlet-class>com.ex.FirstLoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>WelcomeServlet</servlet-name>
    <servlet-class>com.ex.WelcomeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FirstServlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>WelcomeServlet</servlet-name>
    <url-pattern>/welcome</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>LoginPage.jsp</welcome-file>
  </welcome-file-list>
  <session-config>
    <session-timeout>15</session-timeout>
  </session-config>
</web-app>